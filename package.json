{
  "name": "myportfolio",
  "version": "1.0.0",
  "description": "portfolio site with case studies",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "webpack --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod:js": "webpack -p",
    "prod:image": "imagemin assets/images/* --out-dir=assets/images/prod",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "standard 'src/**/*.js'  --verbose --fix | snazzy",
    "lint:sass": "node_modules/sass-lint/bin/sass-lint.js './src/**/*.sass'  -v -q",
    "deploy": "aws s3 sync . s3://aws-website-portfolio-yjme8 --exclude '*.DS_Store*' --exclude '*' --include 'assets/*' --include 'index.html'"
  },
  "repository": {
    "url": "git@github.com:MaartenGDev/myportfolio.git",
    "type": "git"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "author": "MaartenGDev",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "babel-preset-env": "^1.4.0",
    "helmet": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "handlebars": "^4.0.6",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.0.5",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "standard": "^10.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "webpack": "^2.5.1"
  }
}
